library(broom)
set.seed(100)
dat <- tibble(
N = 100,
x1 = rnorm(N),
x2 = rnorm(N, 1, 10),
x3 = rnorm(N, 10, 3),
y = x1 + x2 + runif(N)
)
cols_to_use <- c("x1", "x2", "x3")
predictors <- accumulate(cols_to_use, function(a,b){paste(a,b, sep=" + ")})
formulas <- paste("y~", predictors)
mods <- NULL
